    "@CLANG_ACC_TEST_EXE_NVPTX64@" in ['True', 'true', '1']
config.clang_acc_test_exe_x86_64_triple = 'x86_64-unknown-linux-gnu';
config.clang_acc_test_exe_nvptx64_triple = 'nvptx64-nvidia-cuda';

# Let the main config do the real work.
lit_config.load_config(config, "@LIBOMP_BASE_DIR@/test/lit.cfg")
